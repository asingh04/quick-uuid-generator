{
  "name": "uuid-gen-extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run cleanup && tsc -b && vite build && npm run zipp",
    "lint": "eslint .",
    "cleanup": "rm -rf dist && rm -f quick-uuid-generator.zip",
    "preview": "vite preview",
    "zipp": "cd dist && zip -vr ../quick-uuid-generator.zip . -x \\*.DS_Store"
  },
  "dependencies": {
    "@ant-design/icons": "5.5.2",
    "antd": "5.22.2",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "9.15.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "globals": "15.12.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.15.0",
    "vite": "5.4.11"
  }
}
